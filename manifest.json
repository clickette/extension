{
   "action": {
      "default_icon": "icons/icon128.png",
      "default_popup": "src/browser_action/browser_action.html",
      "default_title": "Quick Search Page"
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "https://clickette.net/assets/favicon.ico",
         "is_default": true,
         "keyword": "clickette.net",
         "name": "Clickette",
         "search_url": "https://clickette.net/search?q={searchTerms}"
      }
   },
   "chrome_url_overrides": {
      "newtab": "src/override/override.html"
   },
   "default_locale": "en",
   "description": "Looking to escape the trackers? Clickette is your #1 solution to stay private online.",
   "homepage_url": "https://clickette.net/",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 3,
   "name": "Clickette",
   "version": "1.1"
}
